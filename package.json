{
  "name": "mern-boilerplate",
  "version": "1.0.0",
  "description": "A boiler plate for Mongo DB, Express, React and Node stack, This includes basic authentication and basic route protection.",
  "main": "app.js",
  "engines": {
    "node": "12.17.0",
    "npm": "6.14.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clinsmann/mern-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Clinsmann/mern-boilerplate/issues"
  },
  "homepage": "https://github.com/Clinsmann/mern-boilerplate#readme",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.16",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^3.1.1",
    "validator": "^13.1.1"
  }
}
